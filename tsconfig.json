{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "sourceMap": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "jsx": "preserve",
    "lib": ["esnext", "dom"],
    "types": [
      "vite/client",
      "jest",
      "node",
      "nuxt3/types",
      "./nuxt",
      "./docs/.nuxt/auto-imports"
    ],
    "rootDir": ".",
    "paths": {
      "@fastkit/*": ["packages/*/src"],
      "fastkit/*": ["packages/fastkit/src"],
      "~~": [
        "./docs"
      ],
      "~~/*": [
        "./docs/*"
      ],
      "@@": [
        "./docs"
      ],
      "@@/*": [
        "./docs/*"
      ],
      "~": [
        "./docs"
      ],
      "~/*": [
        "./docs/*"
      ],
      "@": [
        "./docs"
      ],
      "@/*": [
        "./docs/*"
      ],
      "assets": [
        "./docs/assets"
      ],
      "public": [
        "./docs/public"
      ],
      "public/*": [
        "./docs/public/*"
      ],
      "#app": [
        "./node_modules/nuxt3/dist/app"
      ],
      "#app/*": [
        "./node_modules/nuxt3/dist/app/*"
      ],
    }
  },
  "include": [
    "packages/global.d.ts",
    "packages/*/src",
    "core",
    "docs",
    "docs/.vui/vui.d.ts"
  ]
}
