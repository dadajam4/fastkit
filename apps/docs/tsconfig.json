{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "node",
      "@fastkit/plugboy/env",
      "@fastkit/ts-tiny-meta/client",
      "vite/client",
      "./.vui/vui"
    ],
    "paths": {
      "@@@": [
        "../.."
      ],
      "@@@/*": [
        "../../*"
      ],
      "@@": [
        "./modules"
      ],
      "@@/*": [
        "./modules/*"
      ],
      "~/*": [
        "./src/*"
      ],
    }
  }
}
