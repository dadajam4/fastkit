{
  "name": "@fastkit/vui",
  "version": "0.12.6",
  "description": "A simple, extensible UI kit for Vue applications.",
  "buildOptions": {
    "name": "Vui",
    "tool": true
  },
  "_docs": {
    "scope": "vue",
    "feature": "ui",
    "description": {
      "en": "A simple, extensible UI kit for Vue applications.",
      "ja": "Vueアプリケーション用のシンプルで拡張可能なUIキット。"
    }
  },
  "main": "./dist/vui.mjs",
  "exports": {
    "./vui.css": "./dist/vui.css",
    "./tool": {
      "import": "./dist/tool/index.mjs",
      "types": "./dist/tool/index.d.ts"
    },
    ".": "./dist/vui.mjs"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/vui.d.ts"
      ],
      "tool": [
        "./dist/tool/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/vui.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "keywords": [
    "fastkit",
    "vue",
    "ui",
    "component"
  ],
  "author": "dadajam4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "homepage": "https://github.com/dadajam4/fastkit/tree/dev/packages/vui#readme",
  "peerDependencies": {
    "vite": "^4.0.4",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@fastkit/color-scheme": "0.12.6",
    "@fastkit/icon-font": "0.12.6",
    "@fastkit/tiny-logger": "0.12.6",
    "@fastkit/vite-kit": "0.12.6",
    "@fastkit/vue-kit": "0.12.6",
    "@tiptap/extension-link": "^2.0.0-beta.209",
    "@tiptap/extension-text-style": "^2.0.0-beta.209",
    "@tiptap/extension-underline": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.209",
    "@tiptap/vue-3": "^2.0.0-beta.209",
    "eta": "^1.12.3",
    "fs-extra": "^11.1.0",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.3",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-view": "^1.29.1"
  }
}
