{
  "name": "@fastkit/vue-location",
  "version": "0.6.0",
  "description": "vue-location",
  "keywords": [
    "vue",
    "vue-router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/vue-location.d.mts",
      "import": {
        "default": "./dist/vue-location.mjs"
      }
    },
    "./*": "./dist/*"
  },
  "main": "./dist/vue-location.mjs",
  "types": "./dist/vue-location.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/vue-location.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "plugboy build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "pnpm run eslint:fix",
    "lint": "pnpm run eslint",
    "stub": "plugboy stub",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastkit/vue-utils": "workspace:^"
  },
  "devDependencies": {
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "_docs": {
    "scope": "vue",
    "feature": "",
    "description": {
      "en": "Custom classes for Type-safe handling of exceptions in your application.",
      "ja": "あなたのアプリケーションで例外をTypeセーフに取り扱うためのカスタムクラス。"
    }
  }
}