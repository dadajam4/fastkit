{
  "name": "@fastkit/ts-tiny-meta",
  "version": "1.1.0",
  "description": "ts-tiny-meta",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/ts-tiny-meta.d.ts",
      "import": {
        "default": "./dist/ts-tiny-meta.mjs"
      }
    },
    "./ts": {
      "types": "./dist/ts.d.ts",
      "import": {
        "default": "./dist/ts.mjs"
      }
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": {
        "default": "./dist/vite.mjs"
      }
    },
    "./ts-morph": {
      "types": "./dist/ts-morph.d.ts",
      "import": {
        "default": "./dist/ts-morph.mjs"
      }
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/ts-tiny-meta.mjs",
  "types": "./dist/ts-tiny-meta.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/ts-tiny-meta.d.ts"
      ],
      "ts": [
        "./dist/ts.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ],
      "ts-morph": [
        "./dist/ts-morph.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "plugboy build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "pnpm run eslint:fix",
    "lint": "pnpm run eslint",
    "stub": "plugboy stub",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.2",
    "joycon": "^3.1.1",
    "ts-morph": "^27.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "vite": "^7.1.7"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}