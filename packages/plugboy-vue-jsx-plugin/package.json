  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/plugboy-vue-jsx-plugin.d.ts",
      "import": {
        "default": "./dist/plugboy-vue-jsx-plugin.mjs"
      }
    },
    "./*": "./dist/*"
  },
  "main": "./dist/plugboy-vue-jsx-plugin.mjs",
  "types": "./dist/plugboy-vue-jsx-plugin.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/plugboy-vue-jsx-plugin.d.ts"
      ]
