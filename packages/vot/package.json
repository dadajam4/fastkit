{
  "name": "@fastkit/vot",
  "version": "0.12.0",
  "description": "Orchestration tools for building Vue applications.",
  "buildOptions": {
    "name": "Vot",
    "tool": true,
    "hooks": {
      "after": [
        "./scripts/after.ts"
      ]
    }
  },
  "_docs": {
    "scope": "vot",
    "feature": "framework",
    "description": {
      "en": "Orchestration tools for building Vue applications.",
      "ja": "Vueアプリケーションを構築するためのオーケストレーションツール。"
    }
  },
  "main": "./dist/vot.mjs",
  "exports": {
    "./tool": {
      "import": "./dist/tool/index.mjs",
      "types": "./dist/tool/index.d.ts"
    },
    "./bin/*": "./bin/*.mjs",
    ".": "./dist/vot.mjs"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/vot.d.ts"
      ],
      "tool": [
        "./dist/tool/index.d.ts"
      ]
    }
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "vot": "./bin/vot.mjs"
  },
  "types": "dist/vot.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "keywords": [
    "fastkit",
    "vot"
  ],
  "author": "dadajam4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "homepage": "https://github.com/dadajam4/fastkit/tree/dev/packages/vot#readme",
  "dependencies": {
    "@fastkit/helpers": "0.12.0",
    "@fastkit/vue-page": "0.12.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@types/http-proxy": "^1.17.7",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/server-renderer": "^3.2.0",
    "@vueuse/head": "^1.0.22",
    "chalk": "^5.2.0",
    "connect": "^3.7.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "vite": "^4.0.4",
    "vite-plugin-pages": "^0.28.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  }
}
