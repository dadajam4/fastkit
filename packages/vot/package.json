{
  "name": "@fastkit/vot",
  "version": "1.2.0",
  "description": "Orchestration tools for building Vue applications.",
  "keywords": [
    "fastkit",
    "vot"
  ],
  "homepage": "https://github.com/dadajam4/fastkit/tree/dev/packages/vot#readme",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/vot.d.mts",
      "import": {
        "default": "./dist/vot.mjs"
      }
    },
    "./tool": {
      "types": "./dist/tool.d.mts",
      "import": {
        "default": "./dist/tool.mjs"
      }
    },
    "./server": {
      "types": "./dist/server.d.mts",
      "import": {
        "default": "./dist/server.mjs"
      }
    },
    "./*": "./dist/*"
  },
  "main": "./dist/vot.mjs",
  "types": "./dist/vot.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/vot.d.mts"
      ],
      "tool": [
        "./dist/tool.d.mts"
      ],
      "server": [
        "./dist/server.d.mts"
      ]
    }
  },
  "bin": {
    "vot": "./bin/vot.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "plugboy build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "pnpm run eslint:fix",
    "lint": "pnpm run eslint",
    "stub": "plugboy stub",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastkit/helpers": "workspace:^",
    "@fastkit/vue-page": "workspace:^",
    "@fastkit/vue-utils": "workspace:^",
    "@rollup/plugin-replace": "^6.0.3",
    "@types/connect": "^3.4.38",
    "@types/express": "^4.17.23",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^24.7.0",
    "@unhead/ssr": "^2.0.17",
    "@unhead/vue": "^2.0.17",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/server-renderer": "^3.5.22",
    "chalk": "^5.6.2",
    "connect": "^3.7.0",
    "express": "^4.21.2",
    "fs-extra": "^11.3.2",
    "http-proxy": "^1.18.1",
    "vite": "^7.1.9",
    "vite-plugin-pages": "^0.33.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "buildOptions": {
    "name": "Vot",
    "tool": true,
    "hooks": {
      "after": [
        "./scripts/after.ts"
      ]
    }
  },
  "_docs": {
    "scope": "vot",
    "feature": "framework",
    "description": {
      "en": "Orchestration tools for building Vue applications.",
      "ja": "Vueアプリケーションを構築するためのオーケストレーションツール。"
    }
  }
}