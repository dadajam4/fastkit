{
  "name": "@fastkit/sprite-images",
  "version": "1.1.0",
  "description": "Tools for generating sprite images.",
  "keywords": [
    "fastkit",
    "sprite",
    "image"
  ],
  "homepage": "https://github.com/dadajam4/fastkit/tree/main/packages/sprite-images#readme",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/sprite-images.d.mts",
      "import": {
        "default": "./dist/sprite-images.mjs"
      }
    },
    "./*": "./dist/*"
  },
  "main": "./dist/sprite-images.mjs",
  "types": "./dist/sprite-images.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/sprite-images.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "plugboy build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "pnpm run eslint:fix",
    "lint": "pnpm run eslint",
    "stub": "plugboy stub",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastkit/ev": "workspace:^",
    "@fastkit/tiny-logger": "workspace:^",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.2",
    "spritesmith": "^3.5.1"
  },
  "buildOptions": {
    "name": "SpriteImages"
  },
  "_docs": {
    "scope": "",
    "feature": "image",
    "description": {
      "en": "Tools for generating sprite images.",
      "ja": "スプライト画像を生成するためのツール。"
    }
  }
}