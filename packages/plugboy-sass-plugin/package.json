{
  "name": "@fastkit/plugboy-sass-plugin",
  "version": "1.0.0",
  "description": "plugboy-sass-plugin",
  "keywords": [
    "plugboy",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "license": "MIT",
  "author": "dadajam4",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/plugboy-sass-plugin.d.ts",
      "import": {
        "default": "./dist/plugboy-sass-plugin.mjs"
      }
    },
    "./*": "./dist/*"
  },
  "main": "./dist/plugboy-sass-plugin.mjs",
  "types": "./dist/plugboy-sass-plugin.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/plugboy-sass-plugin.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "plugboy build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "eslint": "eslint . --ext ts,tsx,js,vue,html,yaml",
    "eslint:fix": "eslint . --ext ts,tsx,js,vue,html,yaml --fix",
    "format": "pnpm run eslint:fix",
    "lint": "pnpm run eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "esbuild-sass-plugin": "^2.8.0",
    "resolve": "^1.22.2",
    "sass": "^1.61.0"
  },
  "devDependencies": {
    "@fastkit/plugboy": "workspace:*"
  },
  "peerDependencies": {
    "@fastkit/plugboy": "workspace:*"
  }
}