  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/plugboy-vanilla-extract-plugin.d.mts",
      "import": {
        "default": "./dist/plugboy-vanilla-extract-plugin.mjs"
      }
    },
    "./css": {
      "types": "./dist/css.d.mts",
      "import": {
        "default": "./dist/css.mjs"
      }
