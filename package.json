{
  "private": true,
  "name": "fastkit",
  "version": "0.3.7",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite-ssr",
    "gen": "node -r esbuild-register core/bootstrap.ts --target ",
    "build": "node -r esbuild-register core/build.ts",
    "size": "node -r esbuild-register core/build.ts fastkit -p -f global",
    "checkdeps": "node -r esbuild-register core/checkDeps.ts",
    "eslint": "eslint . --ext ts,tsx,js,vue,html,yaml",
    "eslint:fix": "eslint . --ext ts,tsx,js,vue,html,yaml --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,html}\" --ignore-path .gitignore",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,html}\" --ignore-path .gitignore --fix",
    "lint": "run-s eslint stylelint",
    "format": "run-s eslint:fix stylelint:fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "play": "node -r esbuild-register ./play.ts",
    "test": "node -r esbuild-register core/build.ts fastkit -f global -d && jest --runInBand",
    "release": "node -r esbuild-register core/release.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "preinstall": "node ./core/checkYarn.js"
  },
  "types": "test-dts/index.d.ts",
  "tsd": {
    "directory": "test-dts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node core/verifyCommit.js"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,html,yaml}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue,html}": [
      "stylelint --fix"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "dadajam4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "homepage": "https://github.com/dadajam4/fastkit",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "keywords": [
    "fastkit"
  ],
