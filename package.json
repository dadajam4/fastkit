{
  "private": true,
  "name": "fastkit",
  "version": "0.9.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd site && node ../packages/vot/bin/vot",
    "build:docs": "cd site && node ../packages/vot/bin/vot build",
    "serve": "cd site && node ../packages/vot/bin/vot serve",
    "preview": "yarn build:docs && yarn serve",
    "memwatch": "cd site && node ../packages/vot/bin/vot serve --memwatch",
    "gen": "node -r esbuild-register core/bootstrap.ts --target ",
    "build": "node -r esbuild-register core/build.ts",
    "size": "node -r esbuild-register core/build.ts fastkit -p -f global",
    "mdi": "node -r esbuild-register core/mdi.ts",
    "checkdeps": "node -r esbuild-register core/checkDeps.ts",
    "eslint": "eslint . --ext ts,tsx,js,vue,html,yaml",
    "eslint:fix": "eslint . --ext ts,tsx,js,vue,html,yaml --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,html}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,html}\" --fix",
    "lint": "run-s eslint stylelint",
    "format": "run-s eslint:fix stylelint:fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:docs": "tsc --noEmit --skipLibCheck --project ./site",
    "play": "node -r esbuild-register ./play.ts",
    "test": "jest --runInBand",
    "release": "node -r esbuild-register core/release.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "preinstall": "node ./core/checkYarn.js"
  },
  "types": "test-dts/index.d.ts",
  "tsd": {
    "directory": "test-dts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --config .lintstagedrc.js",
    "commit-msg": "node core/verifyCommit.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "dadajam4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadajam4/fastkit.git"
  },
  "homepage": "https://github.com/dadajam4/fastkit",
  "bugs": {
    "url": "https://github.com/dadajam4/fastkit/issues"
  },
  "keywords": [
    "fastkit"
  ],
  "devDependencies": {
    "@airbnb/node-memwatch": "^2.0.0",
    "@datadog/browser-logs": "^4.28.1",
